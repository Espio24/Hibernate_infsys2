<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.TestSuiteEntity" table="test_suite" schema="system_manager">
        <id name="idTestSuite">
            <column name="id_test_suite" sql-type="int(11)"/>
        </id>
        <property name="nameTestSuite">
            <column name="name_test_suite" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>